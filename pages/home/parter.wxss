page{
  background:#F4F4F4;
}
.parter-item{
  width:643rpx;
  background:rgba(255,255,255,1);
  border-radius:20rpx;
  margin:60rpx 0 0 78rpx;
  position:relative;
  padding:25rpx 25rpx 15rpx 211rpx;
  box-sizing: border-box;
}
.parter-img{
  width:218rpx;
  height:243rpx;
  border-radius:20rpx;
  border:1px solid #006A25;
  background:#fff;
  position:absolute;
  bottom:15rpx;
  left:-37rpx;
}
.parter-img>image{
  width:218rpx;
  height:243rpx;
  border-radius:20rpx;
  position:absolute;
  bottom:10rpx;
  right:10rpx;
}
.parter-name{
  font-size:32rpx;
  font-family:SimHei;
  font-weight:400;
  color:rgba(51,51,51,1);
  margin-bottom:38rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.parter-pro{
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(102,102,102,1);
  line-height:22rpx;
  margin-bottom:17rpx;
  vertical-align: middle;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.parter-nodata{
  height:1000rpx;
  display:flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.parter-nodata>image{
  width:160rpx;
  height:160rpx;
}
.parter-nodata>view{
  font-size:28rpx;
  color:#999;
}